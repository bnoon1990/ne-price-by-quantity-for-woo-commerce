{
  "name": "noonelite/ne-price-by-quantity-for-woocommerce",
  "description": "Adds variable pricing by quantity with options for percentage or fixed amount discounts and displays a pricing table on the product page.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "autoload": {
    "psr-4": {
      "Noonelite\\NePriceByQuantityForWoocommerce\\Admin\\": "admin/",
      "Noonelite\\NePriceByQuantityForWoocommerce\\Frontend\\": "frontend/",
      "Noonelite\\NePriceByQuantityForWoocommerce\\Includes\\": "includes/"
    }
  },
  "authors": [
    {
      "name": "Ben Noon",
      "email": "Ben@Nooneliterecruitment.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpcsstandards/phpcsutils": "^1.0",
    "wp-coding-standards/wpcs": "^3.1"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
